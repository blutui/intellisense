{
  "name": "intellisense",
  "version": "0.1.1",
  "publisher": "blutui",
  "displayName": "Blutui IntelliSense",
  "description": "Intelligent Blutui tooling for VS Code",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/blutui/intellisense.git"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "keywords": [
    "blutui",
    "intellisense",
    "autocomplete",
    "vscode"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.test",
        "title": "Do i work"
      }
    ],
    "languages": [
      {
        "id": "html",
        "extensions": [
          ".html"
        ],
        "aliases": [
          "html",
          "HTML"
        ],
        "configuration": "language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/functions.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/filters.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/auto.code-snippets"
      }
    ],
    "grammars": [
      {
        "language": "html",
        "scopeName": "text.html.twig",
        "path": "./syntaxes/blutui.tmLanguage",
        "embeddedLanguages": {
          "source.json": "json",
          "source.css": "css",
          "source.css.scss": "scss",
          "source.js": "javascript",
          "source.ts": "typescript"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.20.15",
    "@types/node-fetch": "^2.5.10",
    "@types/vscode": "^1.55.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "glob": "^7.1.6",
    "js-yaml": "^4.1.0",
    "mocha": "^8.2.1",
    "prettier": "^2.3.1",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.0",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "bent": "^7.3.12",
    "node-fetch": "^2.6.1",
    "vscode-uri": "^3.0.2"
  }
}
