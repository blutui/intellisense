{
  "name": "intellisense",
  "displayName": "Blutui IntelliSense",
  "description": "Intelligent Blutui tooling for VS Code",
  "preview": true,
  "author": "Blutui <team@blutui.com>",
  "license": "MIT",
  "version": "0.1.0",
  "homepage": "https://github.com/blutui/intellisense",
  "bugs": {
    "url": "https://github.com/blutui/intellisense/issues",
    "email": "support@blutui.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blutui/intellisense.git"
  },
  "publisher": "blutui",
  "keywords": [
    "blutui",
    "intellisense",
    "autocomplete",
    "vscode"
  ],
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onLanguage:canvas"
  ],
  "main": "dist/extension/index.js",
  "capabilities": {
    "virtualWorkspaces": false
  },
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "canvas",
        "aliases": [
          "Canvas",
          "canvas"
        ],
        "extensions": [
          ".canvas"
        ],
        "configuration": "./canvas.configuration.json"
      },
      {
        "id": "html",
        "extensions": [
          ".html"
        ],
        "aliases": [
          "html",
          "HTML"
        ],
        "configuration": "language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./src/snippets/snippets.code-snippets"
      },
      {
        "language": "html",
        "path": "./src/snippets/functions.code-snippets"
      },
      {
        "language": "html",
        "path": "./src/snippets/filters.code-snippets"
      },
      {
        "language": "html",
        "path": "./src/snippets/auto.code-snippets"
      }
    ],
    "grammars": [
      {
        "language": "canvas",
        "scopeName": "source.canvas",
        "path": "./syntaxes/Canvas.tmLanguage.json"
      },
      {
        "path": "./src/syntaxes/blutui.tmLanguage",
        "language": "html",
        "scopeName": "text.html.twig",
        "embeddedLanguages": {
          "source.json": "json",
          "source.css": "css",
          "source.css.scss": "scss",
          "source.js": "javascript",
          "source.ts": "typescript"
        }
      }
    ]
  },
  "scripts": {
    "dev": "npm run clean && glob-exec --foreach --parallel \"src/*.ts\" -- \"ncc build {{file}} --watch -o dist/{{file.toString().replace(/^src\\//, '').replace(/\\.ts$/, '')}}\"",
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.20.15",
    "@types/node-fetch": "^2.5.10",
    "@types/vscode": "^1.55.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vercel/ncc": "^0.33.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "glob": "^7.1.6",
    "glob-exec": "^0.1.1",
    "mocha": "^8.2.1",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.0",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "bent": "^7.3.12",
    "node-fetch": "^2.6.1",
    "vscode-uri": "^3.0.2"
  }
}
